@using Task.Models;
@using Task.ViewModels.Home;
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model HomeVm

<!DOCTYPE html>
<html>
<head>
<title>Page Title</title>
</head>
<body>
    
    
    
    <h3>Cars</h3>
   @foreach (Car car in Model.Cars)
    {   
           <p>
               <a asp-controller="home" asp-action="details" asp-route-id="@car.Id">
                 @car.Name
               </a>
           </p>
      
    }
    


   

</body>
</html>