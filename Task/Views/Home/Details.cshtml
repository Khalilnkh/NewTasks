@using Task.Models;
@using Task.ViewModels.Home;
@using Task.Controllers;
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model Car

<!DOCTYPE html>
<html>
<head>
    <title>Page Title</title>
</head>
<body>



    <h3>Car Details</h3>
    <p>@Model.Name</p>
    <p>@Model.Id</p>
    @if (Model.CarModels != null && Model.CarModels.Count > 0)
    {
        @foreach (CarModel carModel in Model.CarModels)
        {
            <p>
                <a asp-controller="home" asp-action="details">
                    Fullname: @carModel.Name @carModel.Date @carModel.Price
                </a>
            </p>
        }

    }





</body>
</html>